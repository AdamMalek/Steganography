<Window x:Class="SteganoGraphyUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SteganoGraphyUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="input" HorizontalAlignment="Left" Height="86" Margin="20,163,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="497"/>
        <Label x:Name="label_Copy" Content="Input" HorizontalAlignment="Left" Margin="20,137,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="path" HorizontalAlignment="Left" Height="23" Margin="10,13,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="497"/>
        <Button x:Name="button1" Content="Write" HorizontalAlignment="Left" Margin="102,137,0,0" VerticalAlignment="Top" Width="205" Click="button1_Click"/>
        <Button x:Name="button1_Copy" Content="Read" HorizontalAlignment="Left" Margin="312,137,0,0" VerticalAlignment="Top" Width="205" Click="button1_Copy_Click"/>
        <TextBox x:Name="out" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="497"/>

    </Grid>
</Window>
